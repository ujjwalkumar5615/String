import java.util.Scanner;

public class Demo {

	public static void main(String[] args) {
		
		Scanner scanner=new Scanner(System.in);
		System.out.println("Enter Any String");
		String sc=scanner.next();
		
		
         for(int i=0;i<sc.length();i++) {
        
        	 char c=sc.charAt(i);
        	 System.out.println(c);
         }
		
	}
	
}
